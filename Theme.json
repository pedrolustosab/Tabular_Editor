{
    "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.124.json",
    "dataColors": [
        "#003049",
        "#669BBC",
        "#F89206",
        "#FDF0D5",
        "#E044A7",
        "#744EC2",
        "#D9B300",
        "#D64550",
        "#3599B8",
        "#DFBFBF",
        "#4AC5BB",
        "#5F6B6D",
        "#FB8281",
        "#F4D25A",
        "#7F898A",
        "#A4DDEE",
        "#FDAB89",
        "#B687AC",
        "#28738A",
        "#A78F8F",
        "#168980",
        "#293537",
        "#BB4A4A",
        "#B59525",
        "#475052",
        "#6A9FB0",
        "#BD7150",
        "#7B4F71",
        "#1B4D5C",
        "#706060",
        "#0F5C55",
        "#1C2325"
    ],
    "foreground": "#6C6C6C",
    "foregroundNeutralSecondary": "#6B6967",
    "name": "Pedro",
    "textClasses": {
        "callout": {
            "color": "#6C6C6C",
            "fontFace": "'Segoe UI', wf_segoe-ui_normal, helvetica, arial, sans-serif",
            "fontSize": 28
        },
        "header": {
            "color": "#6C6C6C"
        },
        "label": {
            "color": "#6C6C6C"
        },
        "title": {
            "color": "#6C6C6C",
            "fontFace": "'Segoe UI', wf_segoe-ui_normal, helvetica, arial, sans-serif",
            "fontSize": 10
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        },
                        "radius": 15,
                        "show": true
                    }
                ],
                "filterCard": [
                    {
                        "$id": "Applied",
                        "foregroundColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        }
                    },
                    {
                        "$id": "Available",
                        "foregroundColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        }
                    }
                ],
                "outspacePane": [
                    {
                        "foregroundColor": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    }
                ],
                "visualHeader": [
                    {
                        "foreground": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        },
                        "show": false
                    }
                ],
                "visualHeaderTooltip": [
                    {
                        "themedTitleFontColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        }
                    }
                ],
                "visualTooltip": [
                    {
                        "actionFontColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        },
                        "themedTitleFontColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        },
                        "themedValueFontColor": {
                            "solid": {
                                "color": "#6C6C6C"
                            }
                        }
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FCFBFC"
                            }
                        },
                        "transparency": 0
                    }
                ]
            }
        },
        "slicer": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        },
                        "show": true,
                        "transparency": 0
                    }
                ],
                "data": [
                    {
                        "mode": "Basic"
                    }
                ],
                "dropShadow": [
                    {
                        "show": false
                    }
                ],
                "general": [
                    {
                        "keepLayerOrder": true,
                        "orientation": 1,
                        "outlineWeight": 0
                    }
                ],
                "items": [
                    {
                        "background": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        },
                        "color": {
                            "solid": {
                                "color": "#666666"
                            }
                        },
                        "outline": "None"
                    }
                ],
                "numericInputStyle": [
                    {
                        "background": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    }
                ],
                "selection": [
                    {
                        "selectAllCheckboxEnabled": false,
                        "singleSelect": true,
                        "strictSingleSelect": false
                    }
                ],
                "slider": [
                    {
                        "secondaryLineColor": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    },
                    {
                        "handleBorderColor": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    },
                    {
                        "handleFillColor": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    },
                    {
                        "color": {
                            "solid": {
                                "color": "#FDFFFF"
                            }
                        }
                    }
                ],
                "title": [
                    {
                        "show": false
                    }
                ],
                "visualHeader": [
                    {
                        "show": false
                    }
                ]
            }
        }
    }
}